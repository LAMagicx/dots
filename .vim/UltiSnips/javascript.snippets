snippet func "function" bA
function $1 ($2) {
	$0
}
endsnippet

snippet xhttp "xhttp request" A
let data = new FormData();
var xhttp = new XMLHttpRequest();
xhttp.open("POST", "$1");
xhttp.onreadystatechange = function() {
	if (this.readyState == 4 && this.status == 200) {
		let res = this.responseText;
		console.log(res);
	}
}
xhttp.send(data);
endsnippet
